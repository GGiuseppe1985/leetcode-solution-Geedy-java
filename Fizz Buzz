class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> hs = new ArrayList<>();
        for(int i = 1; i <= n; i++){
            if(i%3==0 && i%5==0){
                hs.add("FizzBuzz");
            } else if(i%3==0){
                hs.add("Fizz");
            } else if(i%5==0){
                hs.add("Buzz");
            } else {
                hs.add(String.valueOf(i));
            }
        }
        return hs;
    }
}
